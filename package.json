{
    "name": "metapool-bos",
    "version": "0.0.1",
    "description": "MetaPool BOS",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky install",
        "start:testnet": "METAPOOL_BOS_URL=https://test.near.org/pierre-dev.near/widget/MetaPool && open $METAPOOL_BOS_URL && echo \"Preview at: $METAPOOL_BOS_URL\"",
